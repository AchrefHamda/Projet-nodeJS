<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"></script>
    <script>
        document.getElementById('Login').style.display = 'none';
        document.getElementById('LoginOut').style.display = 'block';
    </script> 
    <style>
      body{
        background-color: whitesmoke;
      }
    </style>
    <title>Contact</title>
</head>
<body>
  
      <br><br>
    <form class="d-flex" role="search" method="post" action="/ContactSearch">
        <input class="form-control me-2" type="search" placeholder="Search" name="searchContact" aria-label="Search" id="AA">
        <button class="btn btn-dark" type="submit" id="AA" >Search</button>
     
      </form>

   <table class="table table-secondary">
  <thead>
    <th >#</th>
    <th >first Name</th>
    <th>Last Name</th>
    <th >User Name</th> 
    <th >Email</th>
    <th >Tel</th>
    <th >Ville</th>
    <th >Comment</th>
    <th  >Action</th>

  </thead>
  <tbody>
    <div class="TU" style="margin-top: 40px;">
    <tr>
    {{#each Resultat7}}
   <td>{{this.id}}</td>
    <td>{{this.nom}}</td>
    <td>{{this.prenom}}</td>
    <td>{{this.Nom_U}}</td>
    <td>{{this.email}}</td>
    <td >{{this.Tel}}</td>
    <td >{{this.Ville}}</td>
    <td >{{this.Comment}}</td>
    <td >
    <a href="ViewMessage/{{this.id}}" type="button" class="btn btn-light btn-small"><i class="bi bi-eye-fill"style="color: rgb(104, 66, 6);"></i> View</a><br><br>
   <!-- <a href="{{this.id}}" type="button" class="btn btn-light btn-small"> <i class="bi bi-person-fill-x"style="color: red;"></i> Delete</a>-->

    </td>
    </tr>
    {{/each}}
  </tbody>
</table>
</body>
</html>